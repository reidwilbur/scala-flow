[ Flow : simpleSubFlow
  [ ActionNode : subFlowHead
    com.wilb0t.flow.impl.PassAction
    [ com.wilb0t.flow.impl.PassExit : subFlowMid ]
    [ : subFlowEnd ]
  ]
  [ ActionNode : subFlowMid
    com.wilb0t.flow.impl.FailAction
    [ : subFlowEnd ]
  ]
  [ EndNode : subFlowEnd
    com.wilb0t.flow.impl.PassAction
  ]
]
